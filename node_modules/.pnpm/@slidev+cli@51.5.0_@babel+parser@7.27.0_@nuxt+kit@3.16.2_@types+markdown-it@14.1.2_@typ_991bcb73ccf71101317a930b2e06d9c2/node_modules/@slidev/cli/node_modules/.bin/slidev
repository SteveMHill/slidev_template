#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stevenhill/Projects/slidev-gce-theme/node_modules/.pnpm/@slidev+cli@51.5.0_@babel+parser@7.27.0_@nuxt+kit@3.16.2_@types+markdown-it@14.1.2_@typ_991bcb73ccf71101317a930b2e06d9c2/node_modules/@slidev/cli/bin/node_modules:/Users/stevenhill/Projects/slidev-gce-theme/node_modules/.pnpm/@slidev+cli@51.5.0_@babel+parser@7.27.0_@nuxt+kit@3.16.2_@types+markdown-it@14.1.2_@typ_991bcb73ccf71101317a930b2e06d9c2/node_modules/@slidev/cli/node_modules:/Users/stevenhill/Projects/slidev-gce-theme/node_modules/.pnpm/@slidev+cli@51.5.0_@babel+parser@7.27.0_@nuxt+kit@3.16.2_@types+markdown-it@14.1.2_@typ_991bcb73ccf71101317a930b2e06d9c2/node_modules/@slidev/node_modules:/Users/stevenhill/Projects/slidev-gce-theme/node_modules/.pnpm/@slidev+cli@51.5.0_@babel+parser@7.27.0_@nuxt+kit@3.16.2_@types+markdown-it@14.1.2_@typ_991bcb73ccf71101317a930b2e06d9c2/node_modules:/Users/stevenhill/Projects/slidev-gce-theme/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/stevenhill/Projects/slidev-gce-theme/node_modules/.pnpm/@slidev+cli@51.5.0_@babel+parser@7.27.0_@nuxt+kit@3.16.2_@types+markdown-it@14.1.2_@typ_991bcb73ccf71101317a930b2e06d9c2/node_modules/@slidev/cli/bin/node_modules:/Users/stevenhill/Projects/slidev-gce-theme/node_modules/.pnpm/@slidev+cli@51.5.0_@babel+parser@7.27.0_@nuxt+kit@3.16.2_@types+markdown-it@14.1.2_@typ_991bcb73ccf71101317a930b2e06d9c2/node_modules/@slidev/cli/node_modules:/Users/stevenhill/Projects/slidev-gce-theme/node_modules/.pnpm/@slidev+cli@51.5.0_@babel+parser@7.27.0_@nuxt+kit@3.16.2_@types+markdown-it@14.1.2_@typ_991bcb73ccf71101317a930b2e06d9c2/node_modules/@slidev/node_modules:/Users/stevenhill/Projects/slidev-gce-theme/node_modules/.pnpm/@slidev+cli@51.5.0_@babel+parser@7.27.0_@nuxt+kit@3.16.2_@types+markdown-it@14.1.2_@typ_991bcb73ccf71101317a930b2e06d9c2/node_modules:/Users/stevenhill/Projects/slidev-gce-theme/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/slidev.mjs" "$@"
else
  exec node  "$basedir/../../bin/slidev.mjs" "$@"
fi
